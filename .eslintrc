{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": "airbnb",
  "parserOptions": {
      "sourceType": "module"
  },
  "rules": {
      "array-callback-return": "off",
      "arrow-parens": "off",
      "camelcase": "off",
      "comma-dangle": "off",
      "linebreak-style": "off",
      "no-param-reassign": "off"
  }
}